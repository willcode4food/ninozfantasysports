{
    "name": "ninozfantasysports",
    "private": true,
    "description": "A simple starter to get up and developing quickly with Gatsby",
    "version": "0.1.4",
    "author": "Marc Arbesman <marc@ninozfantasysports.com>",
    "dependencies": {
        "@apollo/client": "^3.3.7",
        "@devhammed/use-cookie": "^1.1.0",
        "@emotion/eslint-plugin": "^11.2.0",
        "@emotion/jest": "^11.2.0",
        "@emotion/react": "^11.1.5",
        "@emotion/styled": "^11.1.5",
        "date-fns": "^2.16.1",
        "dotenv": "^8.2.0",
        "firebase": "^8.2.7",
        "firebase-admin": "^9.1.1",
        "gatsby": "^2.24.50",
        "gatsby-image": "^2.4.16",
        "gatsby-plugin-alias-imports": "^1.0.5",
        "gatsby-plugin-breakpoints": "^1.2.4",
        "gatsby-plugin-emotion": "^5.4.0",
        "gatsby-plugin-manifest": "^2.4.24",
        "gatsby-plugin-offline": "^3.2.24",
        "gatsby-plugin-react-helmet": "^3.3.10",
        "gatsby-plugin-sharp": "^2.6.28",
        "gatsby-plugin-typography": "^2.5.10",
        "gatsby-source-filesystem": "^2.11.0",
        "gatsby-source-graphql": "^2.13.0",
        "gatsby-transformer-remark": "^2.16.0",
        "gatsby-transformer-sharp": "^2.5.13",
        "graphql": "^14.7.0",
        "gray-percentage": "^2.0.0",
        "lint-staged": "^10.5.1",
        "prop-types": "^15.7.2",
        "random-username-generator": "^1.0.4",
        "react": "^17.0.1",
        "react-albus": "^2.0.0",
        "react-avatar-editor": "11.1.0",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "react-hook-form": "^6.8.4",
        "react-hooks-async": "^3.10.1",
        "react-icons": "^4.1.0",
        "react-spinners": "^0.10.4",
        "react-text-mask": "^5.4.3",
        "react-tracking": "^8.1.0",
        "react-typography": "^0.16.19",
        "typeface-fjalla-one": "^1.1.13",
        "typography": "^0.16.19"
    },
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.12.13",
        "@emotion/babel-plugin": "^11.2.0",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.3",
        "@testing-library/user-event": "^12.6.0",
        "babel-jest": "^26.5.2",
        "babel-preset-gatsby": "^0.12.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "eslint": "^7.10.0",
        "eslint-plugin-jest": "^24.1.0",
        "eslint-plugin-react": "^7.21.2",
        "husky": "^5.0.9",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.5.3",
        "prettier": "2.2.1",
        "react-test-renderer": "^17.0.1"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "0BSD",
    "scripts": {
        "build": "gatsby build",
        "dev": "rm -rf ./.cache && yarn && gatsby develop -H 0.0.0.0",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "start": "yarn dev",
        "start-debug": "rm -rf ./.cache && yarn && gatsby develop -H 0.0.0.0 --inspect",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "jest",
        "jest-watch": "jest --watch",
        "test-update-snapshot": "jest --updateSnapshot",
        "lint": "eslint --fix --ext .js,.jsx ./src/**/*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gatsbyjs/gatsby-starter-default"
    },
    "bugs": {
        "url": "https://github.com/gatsbyjs/gatsby/issues"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/setup-test-env.js"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": [
                "lint-staged"
            ]
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "eslint --quiet --fix"
        ],
        "*.{json,md,html}": [
            "prettier --write"
        ]
    }
}
